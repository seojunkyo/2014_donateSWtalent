<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_gravity="right" >

    <RelativeLayout
        android:id="@+id/relativeLayout2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true" >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content" >

            <ImageButton
                android:id="@+id/imgbtn_menu_upstencil"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:background="@drawable/menu_upstencil_push"
                android:onClick="mOnClick" />

            <ImageButton
                android:id="@+id/imgbtn_menu_downstencil"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:background="@drawable/menu_downstencil"
                android:onClick="mOnClick" />

            <ImageButton
                android:id="@+id/imgbtn_menu_background"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:background="@drawable/menu_background"
                android:onClick="mOnClick" />

            <ImageButton
                android:id="@+id/imgbtn_menu_save"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:background="@drawable/menu_save"
                android:onClick="mOnClick" />
        </LinearLayout>
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/relativeLayout1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/relativeLayout2"
        android:layout_alignParentLeft="true"
        android:background="@drawable/stencilmenu_back" >

        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true" >

            <HorizontalScrollView
                android:id="@+id/horizontalScrollView1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" >

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="horizontal" >

                    <ImageButton
                        android:id="@+id/imgbtn_upstencil1"
                        android:layout_width="match_parent"
                        android:layout_height="fill_parent"
                        android:background="@drawable/upstencil_1"
                        android:onClick="mOnClick" />

                    <ImageButton
                        android:id="@+id/imgbtn_upstencil2"
                        android:layout_width="match_parent"
                        android:layout_height="fill_parent"
                        android:background="@drawable/upstencil_2"
                        android:onClick="mOnClick" />

                    <ImageButton
                        android:id="@+id/imgbtn_upstencil3"
                        android:layout_width="match_parent"
                        android:layout_height="fill_parent"
                        android:background="@drawable/upstencil_3"
                        android:onClick="mOnClick" />
                    <ImageButton
                        android:id="@+id/imgbtn_upstencil4"
                        android:layout_width="match_parent"
                        android:layout_height="fill_parent"
                        android:background="@drawable/upstencil_4"
                        android:onClick="mOnClick" />
                    <ImageButton
                        android:id="@+id/imgbtn_upstencil5"
                        android:layout_width="match_parent"
                        android:layout_height="fill_parent"
                        android:background="@drawable/upstencil_5"
                        android:onClick="mOnClick" />
                    <ImageButton
                        android:id="@+id/imgbtn_upstencil6"
                        android:layout_width="match_parent"
                        android:layout_height="fill_parent"
                        android:background="@drawable/upstencil_6"
                        android:onClick="mOnClick" />
                </LinearLayout>
            </HorizontalScrollView>

            <HorizontalScrollView
                android:id="@+id/horizontalScrollView2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" >

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="horizontal" >

                    <ImageButton
                        android:id="@+id/imgbtn_downwear1"
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:background="@drawable/downwear_1"
                        android:onClick="mOnClick" />
                    <!--
                    <ImageButton
                        android:id="@+id/imgbtn_upwear2"
                        android:layout_width="80dp"
                        android:layout_height="80dp"
                        android:background="@drawable/upwear_2"
                        android:onClick="mOnClick" />

                    <ImageButton
                        android:id="@+id/imgbtn_upwear3"
                        android:layout_width="80dp"
                        android:layout_height="80dp"
                        android:background="@drawable/upwear_3"
                        android:onClick="mOnClick" />
                    -->

                    <ImageButton
                        android:id="@+id/imgbtn_downwear2"
                        android:layout_width="match_parent"
                        android:layout_height="fill_parent"
                        android:background="@drawable/downwear_2"
                        android:onClick="mOnClick" />
                </LinearLayout>
            </HorizontalScrollView>

            <HorizontalScrollView
                android:id="@+id/horizontalScrollView3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" >

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal" >

                    <ImageButton
                        android:id="@+id/imgbtn_background1"
                        android:layout_width="80dp"
                        android:layout_height="80dp"
                        android:background="@drawable/preview_back"
                        android:onClick="mOnClick" />

                    <ImageButton
                        android:id="@+id/imgbtn_background2"
                        android:layout_width="80dp"
                        android:layout_height="80dp"
                        android:background="@drawable/preview_back2"
                        android:onClick="mOnClick" />
                           <ImageButton
                        android:id="@+id/imgbtn_background3"
                        android:layout_width="80dp"
                        android:layout_height="80dp"
                        android:background="@drawable/preview_back3"
                        android:onClick="mOnClick" />
                                  <ImageButton
                        android:id="@+id/imgbtn_background4"
                        android:layout_width="80dp"
                        android:layout_height="80dp"
                        android:background="@drawable/preview_back4"
                        android:onClick="mOnClick" />
                                         <ImageButton
                        android:id="@+id/imgbtn_background5"
                        android:layout_width="80dp"
                        android:layout_height="80dp"
                        android:background="@drawable/preview_back5"
                        android:onClick="mOnClick" />
                                                <ImageButton
                        android:id="@+id/imgbtn_background6"
                        android:layout_width="80dp"
                        android:layout_height="80dp"
                        android:background="@drawable/preview_back6"
                        android:onClick="mOnClick" />
                                                <ImageButton
                        android:id="@+id/imgbtn_background7"
                        android:layout_width="80dp"
                        android:layout_height="80dp"
                        android:background="@drawable/preview_back7"
                        android:onClick="mOnClick" />
                    <!--
                    <ImageButton
                        android:id="@+id/imgbtn_background3"
                        android:layout_width="80dp"
                        android:layout_height="80dp"
                        android:background="@drawable/etc_3"
                        android:onClick="mOnClick" />
                    -->

                </LinearLayout>
            </HorizontalScrollView>
        </FrameLayout>
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/relativeLayout3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/relativeLayout1"
        android:layout_alignParentLeft="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentTop="true"
        android:background="@drawable/preview_back" >

        <FrameLayout
            android:id="@+id/frameLayout1"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_centerHorizontal="true" >

            <ImageView
                android:id="@+id/imgView_up"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"      
                android:visibility="invisible" />

        <ImageView
            android:id="@+id/imgView_down"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:visibility="invisible" />

	</FrameLayout>
    </RelativeLayout>

    <include layout="@layout/over" />
    

</RelativeLayout>